rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are wide open and allow anyone to read and write to your database.
    // This is not recommended for production environments.
    // You should implement proper security rules before deploying your app.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
